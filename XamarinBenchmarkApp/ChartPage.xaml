<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns:oxy="clr-namespace:OxyPlot.Xamarin.Forms;assembly=OxyPlot.Xamarin.Forms" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="XamarinBenchmarkApp.ChartPage" Title="Chart Benchmark">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Button Clicked="RenderChart" Text="Render Chart" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" />
            <oxy:PlotView Model="{Binding Model}" VerticalOptions="Center" HorizontalOptions="Center" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>